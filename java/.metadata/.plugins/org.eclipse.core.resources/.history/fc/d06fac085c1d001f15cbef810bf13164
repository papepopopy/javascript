public class StreamFlatMap {
	public static void main(String[] args) {
		String[] str1 = {"홍길동","동길이","동순이"};
		String[] str2 = {"홍길동2","동길이2","동순이2"};
		
		Stream<String[]> stm1 = Stream.of(str1, str2);
		System.out.println("Stream of : ");
		
	}
}
