package sample03;

public class test01 {
	public static void main(String[] args) {
//		N을 입력받은 뒤, 구구단 N단을 출력하는 프로그램을 작성하시오.
		
		//수 입력
		Scanner scanner = new Scanner(System.in);
		int gugudan = scanner.nextInt();
		
		if (1 <= gugudan && gugudan<=9) {
			//true
			for (int i=1; i<=9; i++) {
				System.out.println(gugudan+" * "+i+" = "+i*gugudan);
			}
		}
	}

}
